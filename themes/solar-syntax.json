{
  "name": "Solar Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorLineNumber.foreground": "#6a6a6a",
    "editorLineNumber.activeForeground": "#a0a0a0",
    "editorCursor.foreground": "#d4d4d4",
    "editor.selectionBackground": "#3a4550",
    "editor.selectionHighlightBackground": "#3a455040",
    "editor.wordHighlightBackground": "#3a455050",
    "editor.findMatchBackground": "#5a604080",
    "editor.findMatchHighlightBackground": "#5a604040",
    "editorBracketMatch.background": "#3a455060",
    "editorBracketMatch.border": "#6a7580",
    "editorIndentGuide.background1": "#2a2a2a",
    "editorIndentGuide.activeBackground1": "#4a4a4a",
    "editorWhitespace.foreground": "#3a3a3a",
    "editorRuler.foreground": "#2a2a2a",
    "editorGutter.background": "#1a1a1a",
    "editorWidget.background": "#1a1a1a",
    "editorWidget.border": "#3a3a3a",
    "editorSuggestWidget.background": "#1a1a1a",
    "editorSuggestWidget.border": "#3a3a3a",
    "editorSuggestWidget.foreground": "#d4d4d4",
    "editorSuggestWidget.selectedBackground": "#3a4550",
    "editorHoverWidget.background": "#1a1a1a",
    "editorHoverWidget.border": "#3a3a3a",
    "peekView.border": "#4a5560",
    "peekViewEditor.background": "#1a1a1a",
    "peekViewResult.background": "#1a1a1a",
    "peekViewTitle.background": "#1a1a1a",
    "diffEditor.insertedTextBackground": "#4a805020",
    "diffEditor.removedTextBackground": "#80505020",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.background": "#4a4a4a60",
    "scrollbarSlider.hoverBackground": "#5a5a5a80",
    "scrollbarSlider.activeBackground": "#6a6a6aa0",
    "minimap.background": "#1a1a1a",
    "minimapSlider.background": "#4a4a4a40",
    "list.hoverBackground": "#2a3035",
    "list.focusBackground": "#3a4045",
    "list.activeSelectionBackground": "#3a4550",
    "list.inactiveSelectionBackground": "#2a3540",
    "tree.indentGuidesStroke": "#3a3a3a",
    "input.background": "#1a1a1a",
    "input.border": "#3a3a3a",
    "input.foreground": "#d4d4d4",
    "input.placeholderForeground": "#7a7a7a",
    "inputOption.activeBorder": "#6a7580",
    "dropdown.background": "#1a1a1a",
    "dropdown.border": "#3a3a3a",
    "button.background": "#4a5560",
    "button.foreground": "#e8e8e8",
    "button.hoverBackground": "#5a6570",
    "checkbox.background": "#1a1a1a",
    "checkbox.border": "#4a4a4a",
    "tab.border": "#00000000",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBorder": "#00000000",
    "gitDecoration.modifiedResourceForeground": "#e2b050",
    "gitDecoration.deletedResourceForeground": "#707070",
    "gitDecoration.untrackedResourceForeground": "#ffffff",
    "gitDecoration.ignoredResourceForeground": "#454545",
    "gitDecoration.conflictingResourceForeground": "#d09050",
    "gitDecoration.addedResourceForeground": "#ffffff",
    "gitDecoration.stageModifiedResourceForeground": "#e2b050",
    "gitDecoration.stageDeletedResourceForeground": "#707070",
    "list.highlightForeground": "#ffffff",
    "sideBar.foreground": "#909090",
    "sideBarSectionHeader.foreground": "#d0d0d0",
    "icon.foreground": "#a0a0a0",
    "symbolIcon.folderForeground": "#d0c8b0"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7c8590",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "String escapes",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "Control keywords",
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Methods",
      "scope": ["meta.method.declaration", "entity.name.method"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Variable properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#4fc1ff"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Booleans",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Null/Undefined",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Types and Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Type parameters",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Decorators/Annotations",
      "scope": ["entity.name.function.decorator", "meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Tag attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#c586c0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "YAML Keys",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "Module names",
      "scope": ["entity.name.package", "entity.name.module"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#d19a66",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
